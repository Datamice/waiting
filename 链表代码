//用链表实现两个多项式想加




typedef struct Node* List;
typedef struct Node{
    int coef;
    int value;
    List Next;
}Node;


List MakeEmptyAndInitial(){
    List L;
    L=(List)malloc(sizeof(Node));
    if(L = NULL)
        {
            printf("内存空间不足");
        }
     L->Next = NULL;
    int coef,value;



    List r=L;
    while(scanf("%d %d",&coef,&value) != EOF) {
            List p=(List*)malloc(sizeof(Node));
            p->coef=coef;
            p->value=value;
            r->Next=p;
            r=p;
    }
    r->Next=NULL;
    return L;
}

int main(){
    List L1=MakeEmptyAndInitial();
    for(;L1->Next;L1->Next++)
        printf("%d %d",L1->coef,L1->value);

    return 0;

}
